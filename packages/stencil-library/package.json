{
  "name": "@tommyzki/ui-core",
  "version": "0.2.3",
  "description": "Base components for Tommyzki UI build by Stencil",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/tommyzki/tommyzki.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommyzki/tommyzki-ui"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && stencil build",
    "start": "npm run clean && stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/angular-output-target": "^0.8.4",
    "@stencil/core": "^4.7.0",
    "@stencil/react-output-target": "^0.5.3",
    "@stencil/sass": "^3.0.12",
    "@stencil/vue-output-target": "^0.8.8",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^21.9.0",
    "vue": "^3.4.31"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "3902e470d2836890afaa8f304033f89f9ecb0d2c"
}
