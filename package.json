{
  "name": "@tommyzki/ui",
  "version": "0.0.1",
  "homepage": "https://tommyzki.github.io/tommyzki-ui",
  "description": "A modern, reusable, and fast UI component library built with Stencil â€” by tommyzki.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/tommyzki-ui/tommyzki-ui.esm.js",
  "exports": {
    ".": {
      "import": "./dist/tommyzki-ui/tommyzki-ui.esm.js",
      "require": "./dist/tommyzki-ui/tommyzki-ui.cjs.js"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "postbuild-storybook": "cpx \"www/**/*\" storybook-static/www",
    "deploy-storybook": "gh-pages -d storybook-static",
    "build:storybook:all": "npm run build && npm run build-storybook && npm run deploy-storybook"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@stencil/core": "^4.31.0",
    "@stencil/sass": "^3.2.1",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/experimental-addon-test": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@storybook/web-components": "^8.6.14",
    "@storybook/web-components-vite": "^8.6.14",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "cpx": "^1.5.0",
    "gh-pages": "^6.3.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "lit": "^3.3.0",
    "puppeteer": "^24.3.0",
    "storybook": "^8.6.14",
    "vite": "^6.3.5",
    "vite-plugin-live-reload": "^3.0.4"
  },
  "dependencies": {
    "stencil": "^0.0.5"
  },
  "author": "Tommy Miyazaki",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
